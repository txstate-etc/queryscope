{
  "name": "queryscope",
  "version": "0.1.3",
  "description": "Client QueryScope token build time generator for TypeScript based GraphQL projects",
  "exports": {
    "require": "./dist/lib/index.js"
  },
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && tsc",
    "mocha": "TS_NODE_PROJECT=test/tsconfig.json mocha -r ts-node/register test/**/*.ts --exit",
    "test": "./test.sh",
    "start": "ts-node test/index.ts"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.0.0",
    "mocha": "^9.1.2"
  },
  "dependencies": {
    "@types/node": "^14.14.34",
    "@types/jsonwebtoken": "^8.5.8",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.8",
    "typescript": "^4.0.3",
    "jsonwebtoken": "^8.5.1"
  },
  "author": "Chas Tabor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/txstate-etc/queryscope.git"
  },
  "keywords": [
    "codemod",
    "graphql",
    "transformer",
    "typescript",
    "queryscope"
  ],
  "bugs": {
    "url": "https://github.com/txstate-etc/queryscope/issues"
  },
  "homepage": "https://github.com/txstate-etc/queryscope#readme",
  "files": [
    "dist/**/*"
  ]
}
